
<form method="post" action="/test/saisie">
  <div  class="form-group mb-3">
    <label for="idNumVoiture">Numéro voiture</label>
    <input type="text" class="form-control" name="Id" id="idNumVoiture" aria-describedby="emailHelp" placeholder="numéro voiture">
   </div>

  <div class="form-group mb-3">
    <label for="idNomVoiture">Nom de la voiture</label>
    <input type="text" class="form-control" name="Nom" id="idNomVoiture" placeholder="Nom de la voiture">
  </div>

  
  <div class="form-group mb-3">
    <label for="idMarqueVoiture">Marque de la voiture</label>
    <input type="text" class="form-control" name="Marque" id="idMarqueVoiture" placeholder="Marque de la voiture">
  </div>

  <button type="submit" class="btn btn-primary">Enregistrer</button>
</form>